<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="369dp"
        android:layout_height="500dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="8dp">

        <EditText
            android:id="@+id/editText"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Practica y configuracion"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="0dp" />

        <LinearLayout
            android:layout_width="290dp"
            android:layout_height="20dp"
            android:orientation="horizontal">

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/clicksboton1"
                android:layout_width="142dp"
                android:layout_height="wrap_content"
                android:text="1clic+1s+1click"
                tools:layout_editor_absoluteX="258dp"
                tools:layout_editor_absoluteY="63dp" />

            <TextView
                android:id="@+id/clicksboton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2clic+1s+1click"
                tools:layout_editor_absoluteX="258dp"
                tools:layout_editor_absoluteY="63dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="304dp"
            android:layout_height="59dp"
            android:orientation="horizontal">

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/Player1clic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Player 1 gana"
                tools:layout_editor_absoluteX="19dp"
                tools:layout_editor_absoluteY="82dp" />

            <Button
                android:id="@+id/Player2click"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Player 2 gana"
                tools:layout_editor_absoluteX="239dp"
                tools:layout_editor_absoluteY="82dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="127dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="299dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/practicaplayer1"
                    android:layout_width="31dp"
                    android:layout_height="39dp"
                    android:text="0"
                    android:textSize="24sp"
                    tools:layout_editor_absoluteX="308dp"
                    tools:layout_editor_absoluteY="142dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="139dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="299dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/practicaplayer2"
                    android:layout_width="29dp"
                    android:layout_height="36dp"
                    android:text="0"
                    android:textSize="24sp"
                    tools:layout_editor_absoluteX="308dp"
                    tools:layout_editor_absoluteY="142dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="314dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/clicksboton3"
                android:layout_width="143dp"
                android:layout_height="wrap_content"
                android:text="3clic+1s+1click"
                tools:layout_editor_absoluteX="19dp"
                tools:layout_editor_absoluteY="176dp" />

            <TextView
                android:id="@+id/clicksboton4"
                android:layout_width="119dp"
                android:layout_height="wrap_content"
                android:text="4clic+1s+1click"
                tools:layout_editor_absoluteX="265dp"
                tools:layout_editor_absoluteY="176dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="173dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/clicksboton7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="7clic+1s+1click"
                tools:layout_editor_absoluteX="148dp"
                tools:layout_editor_absoluteY="398dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/Startclick"
                    android:layout_width="178dp"
                    android:layout_height="wrap_content"
                    android:text="Start crono"
                    tools:layout_editor_absoluteX="25dp"
                    tools:layout_editor_absoluteY="195dp" />

                <Button
                    android:id="@+id/Pauseclick"
                    android:layout_width="162dp"
                    android:layout_height="wrap_content"
                    android:text="Pausa crono"
                    tools:layout_editor_absoluteX="151dp"
                    tools:layout_editor_absoluteY="195dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="274dp"
                android:layout_height="33dp"
                android:orientation="horizontal">

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextClock
                    android:id="@+id/practicacrono"
                    android:layout_width="124dp"
                    android:layout_height="match_parent"
                    android:text="00:00:00"
                    android:textSize="24sp"
                    tools:layout_editor_absoluteX="161dp"
                    tools:layout_editor_absoluteY="259dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="26dp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <Space
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <Space
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"></LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="235dp"
                android:layout_height="82dp"
                android:orientation="horizontal">

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <Space
                    android:layout_width="122dp"
                    android:layout_height="35dp"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="133dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/clicksboton5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="5clic+1s+1click"
                        tools:layout_editor_absoluteX="19dp"
                        tools:layout_editor_absoluteY="176dp" />

                    <Button
                        android:id="@+id/cronoResetclick4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Reset crono"
                        tools:layout_editor_absoluteX="267dp"
                        tools:layout_editor_absoluteY="195dp" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"></LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"></LinearLayout>
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="321dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/clicksboton6"
                    android:layout_width="155dp"
                    android:layout_height="wrap_content"
                    android:text="6clic+1s+1click"
                    tools:layout_editor_absoluteX="148dp"
                    tools:layout_editor_absoluteY="398dp" />

                <TextView
                    android:id="@+id/clicksboton11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="7clic+1s+1click"
                    tools:layout_editor_absoluteX="148dp"
                    tools:layout_editor_absoluteY="398dp" />
            </LinearLayout>

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:layout_width="315dp"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/button6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reset Marcador"
                tools:layout_editor_absoluteX="120dp"
                tools:layout_editor_absoluteY="417dp" />

            <Button
                android:id="@+id/button5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Deshacer punto"
                tools:layout_editor_absoluteX="120dp"
                tools:layout_editor_absoluteY="324dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">


        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>